            <string>///Player Wins/Press Space/Game Mode
draw_set_halign(fa_center);

//Draw Winner
if(global.p1points&gt;=global.maxpoints){
    draw_text(room_width/2,room_height/2+16,"player 1 wins!");
    draw_text(room_width/2,room_height/2+32,"press R to restart");
}
else if(global.p2points&gt;=global.maxpoints){
    if(instance_exists(obj_cpuplayer)){
        draw_text(room_width/2,room_height/2+16,"CPU wins!");
    } else{
        draw_text(room_width/2,room_height/2+16,"player 2 wins!");
    }
    draw_text(room_width/2,room_height/2+32,"press R to restart");
}
else if(global.space==false&amp;&amp;instance_exists(obj_ball)){
    draw_text(room_width/2,room_height/2+16,"press space");
}

if (global.first=true){
     draw_text(room_width/2, room_height/2+32, "press P to open the menu");
}

//Draw Center Line
draw_line(-4,room_height/2,room_width+4,room_height/2);

//Draw Gamemode
if(global.drawmode==true){
    if(instance_exists(obj_cpuplayer)){
        draw_text(room_width/2, room_height/2+32, "single player mode");
    } else{
         draw_text(room_width/2, room_height/2+32, "multiplayer mode");
    }
}
